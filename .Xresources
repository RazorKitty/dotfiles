Xautolock.time: 1
Xautolock.locker: xlock -mode blank
Xautolock.notify: 30
Xautolock.notifier: notify-send -t 0 "Screen will lock in 30 seconds"
Xautolock.corners: 0-0+
Xautolock.cornerdelay: 5
Xautolock.cornerredelay: 30
